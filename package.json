{
  "name": "eshf-embeddings",
  "version": "1.0.0",
  "description": "Azure Functions pipeline for processing forum post embeddings",
  "main": "dist/index.js",
  "scripts": {
    "start": "func start",
    "build": "tsc && npm run copy-static",
    "copy-static": "copyfiles -u 1 \"static/**/*\" dist",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "azure-functions",
    "embeddings",
    "openai",
    "pinecone",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@azure/cosmos": "^4.1.0",
    "@azure/functions": "^4.5.0",
    "@azure/identity": "^4.0.0",
    "@azure/storage-queue": "^12.17.0",
    "@pinecone-database/pinecone": "^2.0.1",
    "@sentry/node": "^10.28.0",
    "@sentry/profiling-node": "^10.28.0",
    "openai": "^4.20.0",
    "tiktoken": "^1.0.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
